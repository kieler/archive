<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="de.cau.cs.kieler.kaom.graphiti.diagram.DiagramTypeProvider"
            description="Graphiti diagram type for KAOM models"
            id="de.cau.cs.kieler.kaom.diagramTypeProvider"
            name="KAOM Diagram Type Provider">
         <diagramType
               id="de.cau.cs.kieler.kaom.diagramType">
         </diagramType>
         <imageProvider
               id="de.cau.cs.kieler.kaom.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            description="Graphiti diagram for KAOM models"
            id="de.cau.cs.kieler.kaom.diagramType"
            name="Actor Oriented Modeling(KAOM) Graphiti Diagram"
            type="KAOM">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="de.cau.cs.kieler.kaom.graphiti.diagram.ImageProvider"
            id="de.cau.cs.kieler.kaom.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="de.cau.cs.kieler.kaom.graphiti">
         <propertyCategory
               category="Graphiti"></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="de.cau.cs.kieler.kaom.graphiti">
         <propertyTab
               category="Graphiti"
               id="de.cau.cs.kieler.kaom.graphiti.core"
               label="Core">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="de.cau.cs.kieler.kaom.graphiti">
         <propertySection
               class="de.cau.cs.kieler.kaom.graphiti.features.KaomPropertySection"
               filter="de.cau.cs.kieler.kaom.graphiti.features.KaomPropertyFilter"
               id="de.cau.cs.kieler.kaom.graphiti.core"
               tab="de.cau.cs.kieler.kaom.graphiti.core">
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.cau.cs.kieler.kaom.graphiti.diagram.KaomDiagramEditor"
            contributorClass="de.cau.cs.kieler.kaom.graphiti.diagram.KaomEditorActionBarContributor"
            default="true"
            extensions="kaog"
            icon="icons/KaomDiagramFile.gif"
            id="de.cau.cs.kieler.kaom.graphiti.editor"
            name="Graphiti Diagram">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="de.cau.cs.kieler"
            class="de.cau.cs.kieler.kaom.graphiti.diagram.KaomNewWizard"
            icon="icons/KaomDiagramFile.gif"
            id="de.cau.cs.kieler.kaom.graphiti.wizard"
            name="Actor Oriented Modeling Diagram (Graphiti)">
         <description>
            Creates a Graphiti diagram for Actor Oriented Modeling (KAOM).
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
         <command
            categoryId="org.eclipse.ui.category.file"
            defaultHandler="de.cau.cs.kieler.kaom.graphiti.diagram.ReInitKaomDiagramCommand"
            description="Initialize a Graphiti diagram for the selected KAOM model, overwriting any old &apos;kaog&apos; file."
            id="de.cau.cs.kieler.kaom.graphiti.reinitDiagram"
            name="Initialize KAOM Diagram (Graphiti)">
      </command>
         </extension>
      <extension
         point="org.eclipse.ui.menus">
            <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="de.cau.cs.kieler.kaom.graphiti.reinitDiagram"
               label="Initialize Graphiti Diagram File (kaog)"
               style="push"
               tooltip="Reinitialize the Graphiti diagram for the selected KAOM model, overwriting any old &apos;kaog&apos; file.pos; file.">
           <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                     <or>
                        <test
                              forcePluginActivation="false"
                              property="org.eclipse.core.resources.extension"
                              value="kaom">
                        </test>
                        <test
                              forcePluginActivation="false"
                              property="org.eclipse.core.resources.extension"
                              value="kaot">
                        </test></or>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>

</plugin>
